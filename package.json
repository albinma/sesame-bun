{
  "name": "sesame-bun",
  "version": "1.0.50",
  "scripts": {
    "env": "cp .env.example .env",
    "build:prod": "bun build --target=bun ./src/index.ts --outdir=./dist --root=.",
    "dev": "NOD_ENV=development bun run --watch src/index.ts | pino-pretty",
    "start": "NODE_ENV=production TZ=UTC bun run src/index.ts",
    "test": "NODE_ENV=test TZ=UTC LOG_LEVEL=error bun test --coverage | pino-pretty",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "siwe": "bun run tests/siwe-tester.ts",
    "gen:types": "openapi-typescript data/openapi/openapi.yml -o data/openapi/openapi.d.ts --empty-objects-unknown && prettier --write data/openapi/openapi.d.ts",
    "prepare": "husky install && bun prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "bun": "1.0.3",
    "commander": "11.0.0",
    "compression": "1.7.4",
    "ethers": "6.7.1",
    "express": "4.18.2",
    "express-openapi-validator": "5.0.6",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "joi": "17.10.2",
    "jose": "4.14.6",
    "luxon": "3.4.3",
    "pino": "8.15.1",
    "pino-http": "8.5.0",
    "prisma": "5.3.1",
    "siwe": "2.1.4"
  },
  "devDependencies": {
    "@types/compression": "1.7.3",
    "@types/express": "4.17.18",
    "@types/express-session": "1.17.8",
    "@types/helmet": "4.0.0",
    "@types/jest": "29.5.5",
    "@types/luxon": "3.3.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "bun-types": "latest",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "openapi-typescript": "6.6.2",
    "pino-pretty": "10.2.0",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "supertest": "6.3.3",
    "typescript": "5.2.2"
  },
  "module": "src/index.js"
}
